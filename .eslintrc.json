{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@typescript-eslint/parser",
        "project": [
            "./tsconfig.json"
        ]
    },
    "rules": {
        "@typescript-eslint/prefer-nullish-coalescing": "warn"
    }
}